.main-container-outer#main-container-homepage
  = render 'partials/header'
  = render 'partials/submission_date'
  #load-more-container
    #paginate-container
      /- if @submission_dates
      = react_component('InfinitePagination', {total_pages: @submissions_dates.total_pages, current_page: @submissions_dates.current_page})
  - if @submissions_dates && @submissions_dates.total_pages >= 2 && @submissions_dates.total_pages != @submissions_dates.current_page
    / #paginate-container
    /   = link_to 'Load more songs', nil, id: 'paginate-trigger'