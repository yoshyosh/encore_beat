.main-container-outer#main-container-homepage
  = render 'partials/submission_date'
  .main-side-container
    = render 'partials/social_media_side'
    = render 'partials/player'
  #load-more-container
  #paginate-container
    - if @submissions_dates
      = react_component('InfinitePagination', {total_pages: @submissions_dates.total_pages, current_page: @submissions_dates.current_page, path: '/'})
